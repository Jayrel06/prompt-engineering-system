        "genericAuthType": "httpHeaderAuth",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "text",
              "value": "=GitHub Scraper completed!\n\nRepositories scraped: {{ $json.total_repos }}\nPrompts extracted: {{ $json.total_prompts }}\nTime: {{ $json.timestamp }}\nStatus: {{ $json.status }}"
            }
          ]
        },
        "options": {}
      },
      "id": "send-github-notification",
